{
  "name": "icarus",
  "version": "0.2.14",
  "description": "ICARUS Terminal for Elite Dangerous",
  "scripts": {
    "build": "npm run build:web && npm run build:app && npm run build:service && npm run build:package",
    "build:web": "cd src/web && next build && next export -o ../../build/web",
    "build:app": "node scripts/build-app.js",
    "build:service": "node scripts/build-service.js",
    "build:package": "node scripts/build-package.js",
    "build:clean": "node scripts/build-clean.js",
    "build:assets": "node scripts/build-assets.js && npm run build:data",
    "build:data": "node scripts/build-data.js",
    "build:debug": "npm run build:web && npm run build:debug:app && npm run build:debug:service",
    "build:debug:app": "node scripts/build-app.js --debug",
    "build:debug:service": "node scripts/build-service.js --debug",
    "dev:web": "next src/web",
    "dev:service": "node src/service/main.js --http-server=\"http://localhost:3000\"",
    "lint": "npm run lint:go && npm run lint:javascript",
    "lint:go": "go fmt src/app/main.go",
    "lint:javascript": "npx standard --fix",
    "compress-web-images": "npx imagemin src/web/public/images/ --out-dir src/web/public/images"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "changeexe": "^1.0.2",
    "csvtojson": "^2.0.10",
    "fs-extra": "^10.0.0",
    "imagemin-cli": "^7.0.0",
    "makensis": "^1.1.1",
    "nexe": "^4.0.0-beta.19",
    "next": "^12.0.4",
    "nodemon": "^2.0.15",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "standard": "^16.0.4",
    "svgtofont": "^3.12.9",
    "to-ico": "^1.1.5",
    "upx": "^1.0.6"
  },
  "dependencies": {
    "async-retry": "^1.3.3",
    "axios": "^0.24.0",
    "connect": "^3.7.0",
    "dotenv": "^10.0.0",
    "glob": "^7.1.7",
    "http-proxy": "^1.18.1",
    "lodash.throttle": "^4.1.1",
    "nedb-promises": "^5.0.2",
    "serve-static": "^1.14.1",
    "ws": "^8.2.3",
    "yargs": "^17.2.1"
  }
}
